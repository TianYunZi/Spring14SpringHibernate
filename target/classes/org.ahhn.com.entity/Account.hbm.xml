<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.ahhn.com.entity">
    <class name="Account" table="SH_ACCOUNTS">
        <id name="id" type="java.lang.Integer">
            <column name="A_ID"/>
            <generator class="native"/>
        </id>
        <property name="usename" type="java.lang.String">
            <column name="A_USENAME"/>
        </property>
        <property name="balance" type="java.lang.Integer">
            <column name="A_BALANCE"/>
        </property>
    </class>
</hibernate-mapping>